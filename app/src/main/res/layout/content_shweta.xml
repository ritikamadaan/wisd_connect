<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.compaq.connect.ShwetaAct"
    android:fillViewport="false">

    <FrameLayout
        android:id="@+id/content_frame"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/back">

        <Button
            android:layout_width="215dp"
            android:layout_height="60dp"
        android:text="Primary"
        android:id="@+id/button6"
        android:layout_marginLeft="70dp"
            android:layout_gravity="left|top"
            android:background="@drawable/primary"
            android:textSize="25dp"
            android:textStyle="bold"
            android:textColor="#ffffff"
            android:layout_marginTop="20dp"
            android:onClick="prim" />

        <Button
        android:layout_width="213dp"
        android:layout_height="60dp"
        android:text="Secondary"
        android:id="@+id/button7"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="90dp"
        android:onClick="second"
        android:layout_gravity="left|top"
        android:background="@drawable/secondary"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        />

        <Button

            android:background="@drawable/highersec"

            android:layout_gravity="left|top"
            android:textSize="25dp"
            android:textStyle="bold"
            android:textColor="#ffffff"

            android:layout_width="209dp"
            android:layout_height="60dp"
            android:text="Higher secondary"
            android:id="@+id/button8"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="160dp"
            android:onClick="highsec" />

        <Button
        android:layout_width="206dp"
        android:layout_height="60dp"
        android:id="@+id/button9"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="230dp"
        android:onClick="entrancee"


        android:background="@drawable/entrance"
        android:layout_gravity="left|top"
        android:text="ENTRANCE"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
         />

        <Button
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:text="Graduate"
        android:id="@+id/button10"
        android:layout_marginTop="278dp"
        android:layout_marginLeft="70dp"




        android:background="@drawable/graduate"

        android:layout_gravity="left|top"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
            android:onClick="graduatee" />

        <Button
        android:layout_width="195dp"
        android:layout_height="60dp"
        android:text="post graduate"
        android:id="@+id/button11"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="70dp"

        android:background="@drawable/postgrad"

        android:textSize="23dp"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:layout_gravity="left|top"
            android:onClick="pgraduatee" />

    </FrameLayout>
</RelativeLayout>
